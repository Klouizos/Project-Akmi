#include <stdio.h> 

int main()
{
int sum=0; 
int i=0; 
int h; 
int n;


printf("Define the table size:", h); 
scanf("%d", &h); 

int numbers[h];

for(i=0; i<h; ++i)
{
	printf("Enter the numbers:"); 
	scanf("%d", &n);
	numbers[i]=n;
	sum= sum + numbers[i]; 
	
}
printf("The sum is: %d", sum); 

	return 0;
}
